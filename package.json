{
    "name": "Dashing",
    "version": "1.0.0",
    "description": "Model Factories just like in Laravel",
    "main": "dist/index.js",
    "scripts": {
        "test": "jest --watchAll -b",
        "start": "parcel src/index.ts & yarn test",
        "build": "parcel build src/index.ts"
    },
    "babel": {
        "presets": [
            [
                "env"
            ]
        ],
        "plugins": [
            [
                "transform-object-rest-spread"
            ],
            [
                "transform-runtime"
            ]
        ]
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testEnvironment": "node",
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "author": "Edouard Penin <e.peninb@gmail.com>",
    "license": "ISC",
    "homepage": "https://github.com/isthatcentered/dashing#readme",
    "keywords": [
        "dashing",
        "fixture",
        "model factory",
        "factory",
        "model",
        "auto",
        "generate",
        "states",
        "instance"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/isthatcentered/dashing.git"
    },
    "bugs": {
        "url": "https://github.com/isthatcentered/dashing/issues"
    },
    "devDependencies": {
        "@types/faker": "^4.1.2",
        "@types/jest": "^23.3.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "faker": "^4.1.0",
        "jest": "^23.4.2",
        "lodash.merge": "^4.6.1",
        "parcel-bundler": "^1.9.7",
        "ts-jest": "^23.1.2",
        "typescript": "^3.0.1",
        "@types/lodash.merge": "^4.6.4"
    },
    "dependencies": {}
}
